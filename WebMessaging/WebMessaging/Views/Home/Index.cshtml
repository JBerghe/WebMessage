
@model WebMessaging.Models.MessageCollection
@{
    ViewBag.Title = "Messenger";
}

<div class="row">
    <div class="col-md-4">
        <h2>Messages</h2>
        <p>
             @foreach(var C in Model.Content)
             {
                 <p>@C.DateSent - @C.Message</p>
             }
        </p>
    </div>
</div>